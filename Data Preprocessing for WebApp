  {
   "cell_type": "code",
   "execution_count": 177,
   "id": "7c58a9fb",
   "metadata": {},
   "outputs": [],
   "source": [
    "def Convert(obj):\n",
    "    '''\n",
    "    obj : list of dict\n",
    "    return a list of str out of it\n",
    "    where each str is  the name in obj\n",
    "    '''\n",
    "    L= []\n",
    "    for i in ast.literal_eval(obj):\n",
    "        L.append(i[\"name\"])\n",
    "    return L\n",
    "    \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 178,
   "id": "2960e48b",
   "metadata": {},
