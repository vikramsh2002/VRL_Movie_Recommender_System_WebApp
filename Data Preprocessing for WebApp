   "outputs": [],
   "source": [
    "# Cast working \n",
    "# Cast :  top 5 dictionary fetch then take name out of it\n",
    "# Cast cvt to list of top 5 actors"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 188,
   "id": "db8eb198",
   "metadata": {},
   "outputs": [],
   "source": [
    "def Convert_Cast(obj):\n",
    "    '''\n",
    "    Take object obj as str in list of dict form\n",
    "    return a list of top 5 cast out of this\n",
    "    '''\n",
    "    L=[]\n",
    "    for i in ast.literal_eval(obj)[:5]:  # Top 5 dict \n",
    "        L.append(i[\"name\"])\n",
    "    return L"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 189,
