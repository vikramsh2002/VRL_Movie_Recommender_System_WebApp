   "source": [
    "# Use A Measure of Distance\n",
    "# Euclidean ===> Not good idea for higher dim\n",
    "# Cosine ===> It is good with higher dim\n",
    "#\n",
    "# Similarity is inversely properstional to distannce\n",
    "# use  : cosine_similarity===> 0   to 1\n",
    "# 0 --> No Simi \n",
    "# 1 ---> Most Similar"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 259,
   "id": "a1d65bcb",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.metrics.pairwise import cosine_similarity,cosine_distances"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 354,
   "id": "7a1452bf",
   "metadata": {},
   "outputs": [
